        "pattern": "([\\d.a-z]+) Server.*\\(ProFTPD",
        "version": 1
    },
    "vsftpd": {
        "pattern": "vsFTPd(?:\\s+(?:v|(?:version\\s+))?([\\d.]+))?",
        "version": 1
    },
    "Pure-FTPd": {
        "pattern": "Pure-FTPd"
    },
    "Serv-U": {
        "pattern": "Serv-U FTP Server(?:\\s+(?:v|(?:version\\s+))?([\\d.]+))?",
        "version": 1
    },
