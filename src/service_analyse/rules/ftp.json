{
    "bftpd": {
        "pattern": "bftpd(?:\\s+([\\d.]+))?",
        "version": 1
    },
    "Microsoft FTP": {
        "pattern": "Microsoft FTP"
    },
    "WFTPD": {
        "pattern": "WFTPD(?:\\s+([\\d.]+))?",
        "version": 1
    },
    "FileZilla": {
        "pattern": "FileZilla Server(?:\\s+(?:v|(?:\\S+\\s+))?([\\d.]+))?",
        "version": 1
    },
    "DiskStation": {
        "pattern": "DiskStation"
    },
